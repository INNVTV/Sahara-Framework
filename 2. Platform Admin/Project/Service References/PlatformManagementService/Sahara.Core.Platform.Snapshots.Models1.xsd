      <xs:element minOccurs="0" maxOccurs="unbounded" name="AccountClosure" nillable="true" type="tns:AccountClosure" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfAccountClosure" nillable="true" type="tns:ArrayOfAccountClosure" />
