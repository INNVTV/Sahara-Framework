  <xs:complexType name="WorkerSnapshot">
    <xs:sequence>
      <xs:element minOccurs="0" name="FrequencyDescription" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="FrequencyMilliseconds" type="xs:int" />
