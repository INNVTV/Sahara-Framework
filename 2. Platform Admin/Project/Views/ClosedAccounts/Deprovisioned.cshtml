            <td>
                @Html.DisplayFor(modelItem => item.CreatedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Verified)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VerifiedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Provisioned)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProvisionedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastPaymentDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NextPaymentDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TrialEndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CardNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CardName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClosureDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeAgo)
            </td>
            <td>
